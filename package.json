{
  "name": "resume",
  "version": "2.0.0",
  "private": true,
  "homepage": "/resume",
  "dependencies": {
    "@emailjs/browser": "^4.1.0",
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@fontsource/roboto": "^5.0.8",
    "@mui/icons-material": "^5.15.10",
    "@mui/material": "^5.15.10",
    "material-ui": "^0.19.2",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-anime": "^0.3.4",
    "react-dom": "^18.2.0",
    "react-material-ui-form-validator": "^1.0.0",
    "react-responsive": "^4.0.3",
    "react-scripts": "^5.0.1",
    "react-scroll": "^1.6.5",
    "react-toastify": "^4.5.2"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "npm run build-html && npm run build-pdf && npm run build-seo && react-scripts build",
    "build-html": "cd src && resumed render -o ../public/cv.html -t jsonresume-theme-even && cd ..",
    "build-pdf": "html-export-pdf-cli ./public/cv.html -o ./public/cv.pdf",
    "build-seo": "node buildSeo.js",
    "test": "CI=true react-scripts test env=jsdom",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "eject": "react-scripts eject"
  },
  "hooks": {
    "pre-commit": "pretty-quick --staged"
  },
  "devDependencies": {
    "eslint-plugin-prettier": "^3.0.1",
    "gh-pages": "^6.1.1",
    "html-export-pdf-cli": "^1.1.5",
    "husky": "^1.3.1",
    "jsonresume-theme-even": "^0.22.0",
    "npm-run-all": "^4.1.1",
    "prettier": "^1.16.4",
    "pretty-quick": "^1.10.0",
    "pug": "^2.0.4",
    "puppeteer": "^2.0.0",
    "resumed": "^3.0.1",
    "sass": "^1.71.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
